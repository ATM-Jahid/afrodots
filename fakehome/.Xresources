! ! Font
*.font: Fira Code:pixelsize=12
! ! Special
*.background:  #001e26
*.foreground:  #9cc2c3
*.cursorColor: #f34a00
! ! Black
*.color0:      #002831
*.color8:      #006488
! ! Red
*.color1:      #d11c24
*.color9:      #f5163b
! ! Green
*.color2:      #6cbe6c
*.color10:     #51ef84
! ! Yellow
*.color3:      #a57706
*.color11:     #b27e28
! ! Blue
*.color4:      #2176c7
*.color12:     #178ec8
! ! Magenta
*.color5:      #c61c6f
*.color13:     #e24d8e
! ! Cyan
*.color6:      #259286
*.color14:     #00b39e
! ! White
*.color7:      #eae3cb
*.color15:     #fcf4dc
! ! Bold, Italic, Underline
*.colorBD:     #b5d5d3
! ! Opacity
*.alpha: 0.8

! ! Some sensible defaults
Xft.dpi: 101
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb
